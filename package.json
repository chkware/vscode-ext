{
  "name": "chkware",
  "displayName": "chkware",
  "description": "chkware (pronounced as /check:ware/) extension for Visual Studio Code",
  "version": "0.1.0",
  "publisher": "chkware",
  "license": "MPL-2.0",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/chkware/vscode-ext.git"
  },
  "bugs": {
    "url": "https://github.com/chkware/vscode-ext/issues"
  },
  "engines": {
    "vscode": "^1.69.0"
  },
  "preview": true,
  "galleryBanner": {
    "color": "#FFF",
    "theme": "light"
  },
  "extensionKind": [
    "ui"
  ],
  "categories": [
    "Programming Languages",
    "Debuggers",
    "Linters",
    "Formatters",
    "Other",
    "Testing"
  ],
  "activationEvents": [
    "onCommand:chkware.getExamples"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "chkware.getExamples",
        "title": "Get Examples",
        "category": "chkware"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --ext .ts,.tsx --fix .",
    "prettier": "prettier --check './**/*.{js,jsx,ts,tsx,css,md,json}'",
    "prettier:fix": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}'",
    "test": "node ./out/test/runTest.js",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.69.0",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "glob": "^8.0.3",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "chkware",
    "testing",
    "Programming Languages",
    "Debuggers",
    "Linters",
    "Formatters"
  ]
}
